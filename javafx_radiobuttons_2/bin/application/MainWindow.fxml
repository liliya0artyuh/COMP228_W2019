<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="application.MainController">
  <!-- TODO Add Nodes -->
  <children>
    <GridPane layoutX="0.0" layoutY="0.0">
      <children>
        <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="0" GridPane.columnSpan="1" GridPane.rowIndex="2" GridPane.rowSpan="4">
          <children>
            <RadioButton mnemonicParsing="false" text="Female">
              <toggleGroup>
                <ToggleGroup fx:id="genderToggle" />
              </toggleGroup>
              <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" fx:id="x1" />
              </VBox.margin>
            </RadioButton>
            <RadioButton mnemonicParsing="false" text="Male" toggleGroup="$genderToggle" VBox.margin="$x1" />
            <RadioButton mnemonicParsing="false" text="Other" toggleGroup="$genderToggle" VBox.margin="$x1" />
          </children>
          <GridPane.margin>
            <Insets left="5.0" />
          </GridPane.margin>
        </VBox>
        <Label text="Select Gender" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="1">
          <GridPane.margin>
            <Insets left="10.0" />
          </GridPane.margin>
        </Label>
        <Button mnemonicParsing="false" onAction="#handleButtonClick" text="Button" GridPane.columnIndex="3" GridPane.rowIndex="1" />
        <Label fx:id="lblGender" text="" GridPane.columnIndex="3" GridPane.rowIndex="3" />
      </children>
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      </rowConstraints>
    </GridPane>
  </children>
</AnchorPane>
