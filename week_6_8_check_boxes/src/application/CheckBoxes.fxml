<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="application.MyCheckBoxesController">
  <!-- TODO Add Nodes -->
  <children>
    <GridPane layoutY="0.0" AnchorPane.leftAnchor="10.0">
      <children>
        <CheckBox fx:id="checkBox_1" mnemonicParsing="false" onAction="#handleCheckboxAction" text="Male" GridPane.columnIndex="0" GridPane.rowIndex="0" />
        <CheckBox fx:id="checkBox_2" mnemonicParsing="false" onAction="#handleCheckboxAction" text="Female" GridPane.columnIndex="0" GridPane.rowIndex="1" />
        <CheckBox fx:id="checkBox_3" mnemonicParsing="false" onAction="#handleCheckboxAction" text="Other" GridPane.columnIndex="0" GridPane.rowIndex="2" />
        <Label fx:id="label" text="Label" GridPane.columnIndex="2" GridPane.rowIndex="1" />
      </children>
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      </columnConstraints>
      <padding>
        <Insets />
      </padding>
      <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      </rowConstraints>
    </GridPane>
  </children>
</AnchorPane>
